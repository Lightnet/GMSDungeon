floors_placed++;
//pickup a random direction
dir = choose(1,2,3,4,dir,dir,dir,dir);
//move forward (relative to direction) 1 cell
switch(dir){
    case 1:
        x += 32;
        break;
    case 2:
        y += 32;
        break;
    case 3:
        x -= 32;
        break;
    case 4:
        y -= 32;
        break;
}
