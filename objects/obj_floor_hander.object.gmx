//check for being out the room
if(x &lt; 32){
    x = 32;
}
if(x &gt; room_width - 64){
    x = room_width - 64;
}
if(y &lt; 32){
    y = 32;
}
if(y &gt; room_height - 64){
    y = room_height - 64;
}
//store top-left and bottom-right coners of the map
if(x &lt; x_left){x_left = x};
if(x &gt; x_right){x_right = x};
if(y &lt; y_upper){y_upper = y};
if(y &gt; y_lower){y_lower = y};
//destory if max floors have been placed
//show_debug_message(string(floors_placed));
if(floors_placed == floors_maxplaced){
    wall_p = instance_create(x_left,y_upper,obj_wall_placer);
    //wall_p = instance_create(0,0,obj_wall_placer);
    wall_p.stop_x = x_right;
    wall_p.stop_y = y_lower;
    //show_debug_message("done?");
