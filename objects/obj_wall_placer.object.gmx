            <string>/// step check
//show_debug_message("start wal?");
//store the target id number
target = instance_place(x,y,obj_floor);
//check for neighbour floors and place walls
if(instance_exists(target)){
    if(!place_meeting(x+32,y,obj_floor)){
        instance_create(x+32,y,obj_wall)
    };
    if(!place_meeting(x-32,y,obj_floor)){
        instance_create(x-32,y,obj_wall)
    };
    if(!place_meeting(x,y+32,obj_floor)){
        instance_create(x,y+32,obj_wall)
        };
    if(!place_meeting(x,y-32,obj_floor)){
        instance_create(x,y-32,obj_wall)
        show_debug_message("create");
    };
}
if(x &lt; start_x){
    x = start_x;
}
if(x == stop_x){
    y += 32;
    x = 0;
}else{
    x += 32;
}
if(y &gt; stop_y){
    room_speed = 60;
    instance_destroy();
}
//show_debug_message(string(x)+":"+string(y));
</string>
